<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">

    <persistence-unit name="deliveryEatPersistenceUnit">
        <jta-data-source>java:/deliveryEat</jta-data-source>

        <properties>
            <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver" />
            <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/BeFruit" />
        </properties>
    </persistence-unit>
</persistence>
